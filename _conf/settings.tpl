### Settings file for Broadcaster 1.1 theme ### 

### Site info strings, used in _tpl/header.tpl and _tpl/footer.tpl ###

[Site]
# Your station on Twitter
TwitterName = "Sourcefabric"

# Your station on Facebook
FacebookName = "Sourcefabric"

# Your station contact address
StreetAddress = "<p><b>Sourcefabric GmbH</b><br /> Prinzessinnenstr. 20, Aufgang A<br /> 10969 Berlin<br /> +49 (0) 30 616 29 281</p>"

### Stream info strings, used in _tpl/_html-head.tpl ###

[Stream]
# URL of your Airtime server
AirtimeAPI = "//sourcefabric.airtime.pro"

# URL of your streaming server
StreamingServer = "//sourcefabric.out.airtime.pro"

# Mount point of your Ogg Vorbis stream
StreamA = "sourcefabric_a"

# Mount point of your MP3 stream
StreamB = "sourcefabric_b"

# Port used by your streaming server
StreamPort ="8000"
